{
  "expo": {
    "name": "FieldstonSports",
    "slug": "FieldstonSports",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/eagle.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/eagle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.fieldston.sports"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/eagle.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "android.package.nikash"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "19e4f24b-e1d4-415c-b158-8359ab5cb41e"
      }
    }
  }
}
